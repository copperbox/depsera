{
  "name": "depsera-mock-services",
  "version": "1.0.0",
  "description": "Mock services ecosystem for testing Depsera",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:ui": "npm run dev --prefix ui",
    "build": "tsc",
    "build:ui": "npm run build --prefix ui",
    "preview:ui": "npm run preview --prefix ui",
    "start": "node dist/index.js",
    "start:seed": "node dist/index.js --seed",
    "start:new": "node dist/index.js --new-topology --seed",
    "reset": "node dist/index.js --reset --seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "proactive-deps": "^1.0.0",
    "better-sqlite3": "^11.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
